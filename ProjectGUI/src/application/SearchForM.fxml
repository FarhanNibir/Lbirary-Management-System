<AnchorPane prefHeight="548.0" prefWidth="711.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SearchForMController">
   <children>
      <Pane prefHeight="68.0" prefWidth="36.0" />
