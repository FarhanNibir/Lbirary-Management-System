         <toggleGroup>
            <ToggleGroup fx:id="test" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="checkbox2" layoutX="378.0" layoutY="99.0" mnemonicParsing="false" text="Title &amp; Author" toggleGroup="$test" />
      <Button layoutX="329.0" layoutY="475.0" mnemonicParsing="false" onAction="#switchToMember" text="Back" />
   </children>
</AnchorPane>
